{
  "name": "vocalia",
  "version": "0.1.0",
  "description": "Official Node.js SDK for VocalIA Voice AI Platform",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vocalia",
    "voice",
    "ai",
    "speech",
    "telephony",
    "assistant",
    "chatbot",
    "morocco",
    "darija"
  ],
  "author": "VocalIA <dev@vocalia.ma>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jouiet/VoicalAI.git"
  },
  "homepage": "https://vocalia.ma",
  "bugs": {
    "url": "https://github.com/Jouiet/VoicalAI/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "undici": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
